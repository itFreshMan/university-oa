      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isMonitor=="1"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="exclusivegateway2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow5" name="不批准" sourceRef="exclusivegateway2" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${success=="0"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="批准" sourceRef="exclusivegateway2" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${success=="1"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="usertask2" targetRef="exclusivegateway3"></sequenceFlow>
    <userTask id="usertask4" name="院长" activiti:candidateUsers="${directors}" activiti:formKey="/oa/process/form/leave/actApprove"></userTask>
    <sequenceFlow id="flow10" name="批准" sourceRef="exclusivegateway3" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${success=="1"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="不批准或者批准小于48小时" sourceRef="exclusivegateway3" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${success=="0" ||  (success=="1" && hours <= 48)}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="usertask4" targetRef="endevent1"></sequenceFlow>
